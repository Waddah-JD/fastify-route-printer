{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "nodenext",
    "target": "ES2018",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "allowJs": false,
    "sourceMap": false,
    "declaration": false,
    "skipLibCheck": true
  }
}
